             <property name="styleSheet">
              <string notr="true">/* --- GAYA UMUM UNTUK SEMUA CARD --- */
QFrame {
    background-color: #ffffff;
    border: 1px solid #e0e0e0;
    border-radius: 10px;
}

/* 1. TOTAL PEMESANAN (Warna Hijau - Fresh/Order Baru) */
QFrame#cardPemesanan {
    border-left: 5px solid #27ae60;
}
QFrame#cardPemesanan:hover {
    background-color: #f1f9f4;
    border: 1px solid #27ae60;
}

/* 2. TOTAL TRANSAKSI (Warna Biru - Keuangan/Trust) */
QFrame#cardTransaksi {
    border-left: 5px solid #2980b9;
}
QFrame#cardTransaksi:hover {
    background-color: #f1f7fa;
    border: 1px solid #2980b9;
}

/* 3. TOTAL PRODUK (Warna Oranye - Variasi/Aset) */
QFrame#cardProduk {
    border-left: 5px solid #f39c12;
}
QFrame#cardProduk:hover {
    background-color: #fff9f1;
    border: 1px solid #f39c12;
}

/* 4. TOTAL STOK (Warna Merah/Maroon - Urgensi/Gudang) */
QFrame#cardStok {
    border-left: 5px solid #e74c3c;
}
QFrame#cardStok:hover {
    background-color: #fdf2f1;
    border: 1px solid #e74c3c;
}

/* --- STYLING TEKS DI DALAM CARD --- */
QLabel {
    background: transparent;
    border: none;
}

/* Judul Kategori (Gunakan objectName: lbl_judul) */
QLabel#lbl_judul {
    color: #7f8c8d;
    font-size: 12px;
    font-weight: bold;
    text-transform: uppercase;
}

/* Angka Statistik (Gunakan objectName: lbl_angka) */
QLabel#lbl_angka {
    color: #2c3e50;
    font-size: 20px;
    font-weight: bold;
}</string>
